<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.evaldmcsform1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        tools:context=".MainActivity"
        android:id="@+id/evalform1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/position_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/position_name"
                android:textColor="@color/black"
                android:textStyle="italic"
                android:padding="10sp"
                android:layout_marginTop="50dp"
                />
            <TextView
                android:id="@+id/display_name"
                android:layout_width="242dp"
                android:layout_height="35dp"
                android:text="@string/display_name"
                android:textColor="@color/black"
                android:textStyle="italic"
                android:padding="10sp"
                android:layout_marginTop="50dp"
                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="5sp">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:background="@color/colorBlue"
                        android:textSize="5sp">

                        <TextView
                            android:id="@+id/e1survey1_a"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="MINDSETS AND BEHAVIOR"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:padding="10sp"/>
                    </LinearLayout>

                    <androidx.cardview.widget.CardView
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_margin="5sp"
                        app:cardCornerRadius="10sp"
                        app:cardPreventCornerOverlap="true"
                        app:cardElevation="5sp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/e1survey1_b"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:textStyle="bold"
                                android:layout_margin="5sp"
                                android:text="LEARNING AGILITY (WILLINGNESS TO LEARN AND ADOPT QUICKLY)"/>


                            <TextView
                                android:id="@+id/e1survey1_c"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="Company Required Competency Level (RIGHT PEOPLE)"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <TextView
                                android:id="@+id/e1survey1_d"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_margin="5dp"
                                android:text="HD"
                                android:gravity="center_vertical"
                                android:textColor="@color/black" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="GRADE"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <Spinner
                                android:id="@+id/e1spn_survey1_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5sp"
                                android:textSize="12sp"
                                android:entries="@array/HDlevel"/>

                            <TextView
                                android:id="@+id/e1survey_e"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="COACH/LM OBSERVATION NOTES"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <EditText
                                android:id="@+id/e1survey1_feedback_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="60sp"
                                android:layout_margin="5dp"
                                android:gravity="top"
                                android:textSize="10sp"
                                android:hint="Remarks"
                                android:inputType="textMultiLine" />
                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        app:cardCornerRadius="10dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardElevation="5dp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/e1survey2_a"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:layout_margin="5dp"
                                android:text="RIGHT MB WITH SELF: PASSION TO MAKE A DIFFERENCE AND DELIVER RESULTS (MASIPAG, MATYAGA, DISCIPLINED, COMMITTED) " />

                            <TextView
                                android:id="@+id/e1survey2_b"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="Company Required Competency Level (RIGHT PEOPLE)"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <TextView
                                android:id="@+id/e1survey2_c"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_margin="5dp"
                                android:text="HD"
                                android:gravity="center_vertical"
                                android:textColor="@color/black" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="GRADE"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <Spinner
                                android:id="@+id/e1spn_survey2_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:textSize="10sp"
                                android:entries="@array/HDlevel" />

                            <TextView
                                android:id="@+id/e1survey2_d"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="COACH/LM OBSERVATION NOTES"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <EditText
                                android:id="@+id/e1survey2_feedback_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="100dp"
                                android:layout_margin="5dp"
                                android:gravity="top"
                                android:textSize="10sp"
                                android:hint="Remarks"
                                android:inputType="textMultiLine" />

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        app:cardCornerRadius="10dp"
                        app:cardPreventCornerOverlap="true"
                        app:cardElevation="5dp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/e1survey3_a"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:layout_margin="5dp"
                                android:text="RIGHT MB WITH OTHERS: HIGHLY ENGAGES, PRO-ACTIVE/ INITIATES, SERVICE-ORIENTED, SEEKS FIRST TO UNDERSTAND THEN TO BE UNDERSTOOD, THINKS WIN-WIN" />

                            <TextView
                                android:id="@+id/e1survey3_b"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="Company Required Competency Level (RIGHT PEOPLE)"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <TextView
                                android:id="@+id/e1survey3_c"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_margin="5dp"
                                android:text="HD"
                                android:gravity="center_vertical"
                                android:textColor="@color/black" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="GRADE"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <Spinner
                                android:id="@+id/e1spn_survey3_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:textSize="10sp"
                                android:entries="@array/HDlevel" />

                            <TextView
                                android:id="@+id/e1survey3_d"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="10sp"
                                android:text="COACH/LM OBSERVATION NOTES"
                                android:textStyle="bold"
                                android:layout_margin="5dp" />

                            <EditText
                                android:id="@+id/e1survey3_feedback_dmcs"
                                android:layout_width="match_parent"
                                android:layout_height="100dp"
                                android:layout_margin="5dp"
                                android:gravity="top"
                                android:textSize="10sp"
                                android:hint="Remarks"
                                android:inputType="textMultiLine" />

                        </LinearLayout>

                    </androidx.cardview.widget.CardView>


                </LinearLayout>

            </androidx.core.widget.NestedScrollView>




        </LinearLayout>

    </LinearLayout>

</FrameLayout>